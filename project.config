[project]
	description = Revenge starts from here
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Chityanj
[access "refs/for/refs/*"]
	read = group user/Chityanj
	push = group user/Chityanj
	label-Code-Review = -2..+2 group user/Chityanj
	label-Verified = -1..+1 group user/Chityanj
	rebase = group user/Chityanj
	removeReviewer = group user/Chityanj
	submit = group user/Chityanj
[access "refs/heads/*"]
	read = group user/Chityanj
	create = group user/Chityanj
	push = +force group user/Chityanj
	pushMerge = group user/Chityanj
[access "refs/tags/*"]
	push = group user/Chityanj
	push = +force group user/Chityanj

<?xml version="1.0" encoding="UTF-8"?>
<!-- Repos of RevengeOS -->
<manifest>

  <remote  name="ros"
           fetch="https://github.com/revengeos"
           revision="r11.0"
           sync-j="16" />

  <remote  name="devices"
           fetch="https://github.com/RevengeOS-Devices"
           revision="r11.0" />

  <remote  name="github"
           fetch="https://github.com/" />

  <project path="bionic" name="android_bionic" remote="ros" />

  <!-- build -->
  <project path="build/make" name="android_build" remote="ros" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/soong" name="android_build_soong" remote="ros" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>
  
  <!-- external -->
  <project path="external/ant-wireless/ant_client" name="LineageOS/android_external_ant-wireless_ant_client" remote="github" revision="lineage-18.0" />
  <project path="external/ant-wireless/ant_native" name="LineageOS/android_external_ant-wireless_ant_native" remote="github" revision="lineage-18.0" />
  <project path="external/ant-wireless/ant_service" name="LineageOS/android_external_ant-wireless_ant_service" remote="github" revision="lineage-18.0" />
  <project path="external/ant-wireless/antradio-library" name="LineageOS/android_external_ant-wireless_antradio-library" remote="github" revision="lineage-18.0" />
  <project path="external/ant-wireless/hidl" name="LineageOS/android_external_ant-wireless_hidl" remote="github" revision="lineage-18.0" />
  <project path="external/exfat" name="android_external_exfat" remote="ros" />
  <project path="external/json-c" name="LineageOS/android_external_json-c" remote="github" revision="lineage-17.1" />
  <project path="external/ntfs-3g" name="LineageOS/android_external_ntfs-3g" remote="github" revision="lineage-18.0" />
  <project path="external/RevengeUI-lib" name="RevengeUI-lib" remote="ros" revision="master" />
  <project path="external/SimpleWeather" name="SimpleWeather" remote="ros" revision="master" />
  <project path="external/tinycompress" name="LineageOS/android_external_tinycompress" remote="github" revision="lineage-18.0" />
  
  <!-- device -->
  <project path="device/qcom/sepolicy" name="android_device_qcom_sepolicy" remote="ros" />
  <project path="device/qcom/sepolicy-legacy-um" name="android_device_qcom_sepolicy" remote="ros" revision="r11.0-legacy-um" />
  <project path="device/qcom/sepolicy_vndr" name="android_device_qcom_sepolicy_vndr" remote="ros" />
  <project path="device/revengeos/sepolicy" name="android_device_revengeos_sepolicy" remote="ros" />

  <!-- frameworks -->
  <project path="frameworks/av" name="android_frameworks_av" remote="ros" />
  <project path="frameworks/base" name="android_frameworks_base" remote="ros" />
  <project path="frameworks/native" name="android_frameworks_native" remote="ros" />

  <!-- hardware -->
  <project path="hardware/libhardware" name="android_hardware_libhardware" remote="ros" />
  
  <!-- packages -->
  <project path="packages/apps/Email" name="LineageOS/android_packages_apps_Email" remote="github" revision="lineage-17.1" />
  <project path="packages/apps/UnifiedEmail" name="LineageOS/android_packages_apps_UnifiedEmail" remote="github" revision="lineage-17.1" />
